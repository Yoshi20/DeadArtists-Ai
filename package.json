{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.1.3",
    "@metamask/detect-provider": "^1.2.0",
    "beercss": "~2.1.3",
    "esbuild": "^0.14.53",
    "sass": "^1.54.4",
    "tom-select": "^2.1.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
